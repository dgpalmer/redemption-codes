{#
/**
 * @file redemption.html.twig
 * Default theme implementation to present Redemption data.
 *
 * This template is used when viewing Redemption pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_redemption()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('redemption') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
