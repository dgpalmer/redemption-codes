{#
/**
 * @file redemption_code.html.twig
 * Default theme implementation to present Redemption Code data.
 *
 * This template is used when viewing Redemption Code pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_code()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('code') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
